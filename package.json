{
	"name": "xq-util",
	"version": "1.0.3",
	"description": "前端程序的一个公共方法库，包括：domReady、children、append、find、jsonFormData等公共方法。",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "npx unbuild && npx xq-banner"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/xqkeji/xq-util.git"
	},
	"keywords": [
		"xq-util",
		"domReady",
		"children",
		"parents",
		"append",
		"find",
		"jsonFormData"
	],
	"author": "xqkeji.cn",
	"license": "SSPL-1.0",
	"homepage": "http://xqkeji.cn/",
	"devDependencies": {
		"@types/node": "^18.13.0",
		"typescript": "^4.9.5",
		"unbuild": "^1.1.1",
		"xq-banner": "^1.0.8"
	}
}
